{
    "python.formatting.provider": "black",
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true,
        "**/Thumbs.db": true,
        "**/__pycache__": true,
        "**/.mypy_cache": true,
        "venv": true,
        ".pytest_cache": true,
        "frontend/.cache": true,
        "frontend/**/build": true,
        "frontend/node_modules": true
    }
}